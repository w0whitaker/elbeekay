---
layout: layouts/page.njk
title: "Projects."
permalink: /projects/
---

{# loop over each post in the projects collection #}
<ul class="layout:[ flex flex-col gap-xl ]">
  {% for project in collections.projects %}
    <li>
      <div class="layout:[ flex justify-between items-center ]">
        <a href="{{ project.url }}" class="layout:[ align-middle ]">{{ project.data.title }}</a>
        <img src="/4-3-land.svg" alt="placeholder" class="aesthetic:[ placeholder ]" />
      </div>
    </li>
  {% endfor %}
</ul>