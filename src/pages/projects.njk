---
layout: layouts/page.njk
title: "Projects."
permalink: "/projects/"
---
{% for project in projects %}
  <ul>
    <li>
      <h3>
        <a href="{{ project.name | slugify }}/">{{ project.name }}</a>
      </h3>
      <p>{{ project.description }}</p>
      <img src="{{ project.thumbnail.url }}" alt="{{ project.thumbnail.alt }}" class="placeholder" >
    </li>
  </ul>
{% endfor %}