{
  "name": "elbeekay",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf _site",
    "start": "npm-run-all --parallel dev:*",
    "build": "run-s clean build:*",
    "dev:eleventy": "npx @11ty/eleventy --serve --incremental --watch",
    "dev:css": "tailwindcss -i ./src/assets/css/main.css -o ./_site/assets/css/main.css --watch --postcss",
    "build:eleventy": "npx @11ty/eleventy",
    "build:css": "tailwindcss -i ./src/assets/css/main.css -o ./_site/assets/css/main.css --postcss",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-vite": "^4.0.0",
    "lit": "^2.6.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-nesting": "^11.2.2",
    "prettier": "2.8.8",
    "rimraf": "^5.0.0",
    "stylelint": "^15.6.1",
    "stylelint-config-standard": "^33.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2"
  }
}
